{
    "components": {
        "app": {
            "style": {
                "background-color": "black",
                "color": "white",
                "font-family": "'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif",
                "width": "100%",
                "height": "100%",
                "overflow": "auto"
            },
            "tagName": "div",
            "components": {
                "devices": {
                    "tagName": "div",
                        "components": {
                            "devicediv": {
                                "tagName": "div",
                                "components": {
                                        "connectheader": {
                                            "src": "../../plugins/connect/header.js"
                                        },
                                        "connectmode": {
                                            "src": "../../plugins/connect/mode.js"
                                        },
                                        "selectUSB": {
                                            "src": "../../plugins/select/usb.js"
                                        },
                                        "selectBLE": {
                                            "src": "../../plugins/select/ble.js"
                                        },
                                        "connectDevice": {
                                            "src": "../../plugins/connect/device.js"
                                        }
                                }
                            }
                    }
                },
                "output": {
                    "tagName": "div",
                        "components": {
                            "playsounds": {
                                "tagName": "div",
                                "components": {
                                        "soundheader": {
                                            "src": "../../plugins/output/sound/header.js"
                                        },
                                        "soundDropdown": {
                                            "src": "../../plugins/output/sound/dropdown.js",
                                            "soundFilePaths": [
                                                {
                                                    "label": "Kalimba",
                                                    "src": "./src/assets/kalimba.wav"
                                                },
                                                {
                                                    "label": "Phonk",
                                                    "src": "./src/assets/phonk.wav"
                                                },
                                                {
                                                    "label": "Synth Flute",
                                                    "src": "./src/assets/synthflute.wav"
                                                }
                                            ]
                                        },
                                        "play": {
                                            "src": "../../plugins/output/sound/play.js"
                                        },
                                        "stop": {
                                            "src": "../../plugins/output/sound/stop.js"
                                        }
                                    }
                            },
                            "other": {
                                "tagName": "div",
                                "components": {
                                        "stats": {
                                            "src": "../../plugins/output/stats/start.js"
                                        },
                                        "resetstats": {
                                            "src": "../../plugins/output/stats/reset.js"
                                        },
                                        "waveform": {
                                            "src": "../../plugins/output/waveform.js"
                                        },
                                        "csvmenu": {
                                            "src": "../../plugins/output/csv/menu.js"
                                        }
                                }
                            }
                        }
                }
            }
        }
    }
